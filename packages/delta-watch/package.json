{
  "name": "delta-watch",
  "version": "0.0.8",
  "description": "Utility for watching objects and separating mutating, watching, and accessing.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "clean": "echo `rm -r core types index.js index.js.map index.d.ts &>/dev/null`",
    "build": "tsc",
    "build-publish": "npm run clean && npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chadrc/delta-watch.git"
  },
  "keywords": [
    "watch",
    "utility"
  ],
  "author": "Chad Collins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chadrc/delta-watch/issues"
  },
  "homepage": "https://github.com/chadrc/delta-watch#readme",
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/jest": "^22.2.3",
    "chai": "^4.2.0",
    "jest": "^22.4.4",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "src/__tests__/.*\\.test\\.(ts|tsx|js)$"
  }
}
